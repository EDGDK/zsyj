.mrdz ul{width:950px; height:33px; margin-top:10px;}
.mrdz ul li{float:left;width:85px;text-align:center; height:39px;  line-height:39px; font-family:'微软雅黑';}
.moren{width:90%;margin-left:50px;}
.moren:hover{background:#ffe7fe;}
.morenChosedUl{background:#ffe7fe;margin-left:50px;}
