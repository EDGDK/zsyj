.reg{font-family:"微软雅黑";font-size:12px;width:245px;height:35px;border-radius:8px;background:#f145b7;color:#FFF;font-weight:600;border:none;outline:medium;float:left;margin-left:150px;margin-top:50px;}
.reg:hover{background:#cc33ff;}

.yonghu{float:left;margin-left:150px;margin-top:-370px;font-size:10px;color:#F6C;}


.mima{float:left;margin-left:150px;margin-top:-320px;font-size:10px;color:#F6C;}
.hint{float:left;margin-left:150px;margin-top:-270px;font-size:10px;color:#F6C;}
