
input:focus {
    outline:solid 1px #3CF;
}
ul{list-style:none;}
a{text-decoration:none;outline:none;}
a,img{border:0;}
@font-face{ font-family:"微软雅黑";  
	    src:url("../../css/home/Yahei.ttc");
}
/*wrapper*/
.wrapper{width:auto;height:auto;margin:0 auto;}
/*header*/
/*content*/
.content{width:100%;min-width:1000px;height:auto;margin:0 auto;}
.banner{width:52.08%;min-width:1000px;height:252px;margin:0 auto;}
.banner1{float:left;margin-top:20px;}
.banner2{float:right;margin-top:20px;margin-right:250px;}
.clear{clear:both;}
.banner_nav{width:130px;height:15px;font-size:14px;color:#d967d9;}
.banner_nav a{color:#d967d9;}
.banner_nav a:hover{color:#ff33ff;}
.banner_nav span{padding:10px 10px;}
.banner_line{ border-bottom:1px solid #9e4897;width:1000px;margin:0 auto;margin-top:10px}
.content_top{width:52.08%;min-width:1000px;height:30px;margin:0 auto;margin-top:10px;line-height:30px;
background: -moz-linear-gradient(left, #ca47c9, #f7affa 60%,#d25ad2);
/* Webkit: Safari 4-5, Chrome 1-9 */
background: -webkit-gradient(linear, 100% 0%, 0% 0%, from(#ca47c9), to(#d25ad2));
/* Webkit: Safari 5.1+, Chrome 10+ */
background: -webkit-linear-gradient(left, #ca47c9, #f7affa 60%,#d25ad2);
/* Opera 11.10+ */
background: -o-linear-gradient(left, #ca47c9, #f7affa 60%,#d25ad2);
/* IE 10 */
background: -ms-linear-gradient(left, #ca47c9, #f7affa 60%,#d25ad2);
/* IE < 10 */ 
/* 注意：这一行必须写在最后 */ 
}
/*颜色渐变*/
.all{float:left;font-size:12px;color:#fff;}
.all img{margin-left:12px;position:relative;bottom:10px;}
.all span{margin-left:12px;}
.me{float:right;font-size:12px;color:#fff;padding-right:22px;}
.me a{color:#fff;}
.me a:hover{color:#ff33ff;}
.article{min-width:1000px;width:100px;height:auto;margin:0 auto;}
.article_one{min-width:996px;height:auto;margin:0 auto;border: solid 2px #b5b5b5;margin-top:20px;}
.floor{width:996px;height:35px;margin:0 auto;background:#f4f4f4;margin-top:8px;line-height:35px;}
.back{height:auto;width:900px;margin-left:90px;margin-bottom:10px;}
.speak_back{width:900px;height:auto;margin:0 auto;margin-top:8px;line-height:35px;}
.speak_back span{float:right;margin-right:70px;font-size:14px;font-family:"宋体";}
.speak_back a{font-size:14px;font-family:"宋体";margin-left:20px;}
.speak_back p{font-size:14px;font-family:"宋体";text-indent: 2em;line-height:1.2;}
.floor_name{float:left;margin-left:20px;font-size:14px;font-family:"宋体";}
.time{float:right;margin-right:30px;font-size:14px;font-family:"宋体";}
p{font-size:14px;font-family:"宋体";text-indent: 2em;line-height:1.2;margin-top:10px;line-height:20px;width:950px;margin:10px auto;}
.article_foot{width:1000px;height:auto;margin:0 auto;background:#ececec;border:1px solid #c6c6c6;margin-top:25px;}
p.remind{float:left;font-size:12px;font-family:"宋体";margin-top:0;line-height:35px;}
p.remind span{padding:5px 5px;}
.number{float:left;font-size:12px;font-family:"宋体";line-height:35px;margin-left:40px;}
.totalnums{float:left;font-size:12px;font-family:"宋体";line-height:35px;margin-left:15px;}
.article_foot ul{width:365px;height:auto;float:left;margin-left:150px;margin-top:-10px;line-height:35px;}
.article_foot ul li{float:left;font-size:12px;font-family:"宋体";margin-left:8px;}
.article_foot ul li a{color:#000;}
.article_foot ul li:nth-child(1) a{color:#fe15fb;}
.article_foot ul li a:hover{color:#fe15fb;}
.skip{float:right;width:120px;height:35px;margin-right:40px;font-size:12px;font-family:"宋体";}
.skip span{float:left;}
.ye input{width:27px;height:20px;background:#fff;float:left;border:1px solid #c6c6c6;margin-left:5px;margin-right:5px;margin-top:-5px;}
.ye{float:left;}
.go{float:right;width:23px;height:18px;border:#a8a8a8 1px solid;background:#f9f9f9;color:#000;line-height:18px;text-align:center;margin-top:-3px;}
.go:hover{color:#F00;}
.leave{min-width:1000px;height:335px;margin:0 auto;}
.leave_top{min-width:1000px;height:23px;margin:0 auto;margin-top:13px;}
.leave_top span{float:left;font-size:20px;font-family:"黑体";color:#913399;}
.leave_top img{margin-top:5px;margin-left:2px;}
.leave form{margin:0 auto;width:490px;height:290px;margin-top:20px;}
.leave_text{height:22px;width:490px;margin:0 auto;margin-top:8px;}
.leave_text .one{font-size:12px;font-family:"宋体";letter-spacing:8px;line-height:1.5;float:left;}
.leave_text input{width:223px;border:1px solid #cccccc;background:#fff;height:20px;line-height:1.5;}
.xing2{float:right;color:#ff0000;padding-right:46px;}
.xing1{float:right;color:#ff0000;padding-right:106px;}
.xing3{float:right;color:#ff0000;padding-right:81px;}
.leave_text span.tishi1{float:right;font-size:12px;font-family:"宋体";color:#8c8c8c;position:relative;right:80px;}
.leave_text span.tishi2{float:right;font-size:12px;font-family:"宋体";color:#8c8c8c;position:relative;right:20px;}
.leave_text span.tishi3{float:right;font-size:12px;font-family:"宋体";color:#8c8c8c;position:relative;right:55px;}
.leave_message{margin-top:8px;margin:0 auto;width:490px;height:230px;}
.leave_message .one{font-size:12px;font-family:"宋体";letter-spacing:8px;line-height:1.5;float:left;margin-top:10px;}
#fortips_error{color:red;font-size:8px;margin-left:60px;}
textarea{width:388px;height:154px;border:2px solid #cccccc;background:#fff;margin-left:60px;overflow: scroll;resize:none; 
scrollbar-face-color: #c339a4;/*滚动条中间可以拉的部分和箭头旁边部分的顏色;*/
scrollbar-shadow-color: #d4d3cf;/*滚动条右侧边框的顏色（用于显示阴影之用）;*/
SCROLLBAR-HIGHLIGHT-COLOR: #404040;/*滚动条左侧边框的顏色（用于显示立体之用）;*/
SCROLLBAR-3DLIGHT-COLOR: #404040;/*滚动条左侧边框的顏色（用于显示立体之用）;*/
scrollbar-darkshadow-color: #85989C;/*滚动条右侧边框的顏色（用于显示阴影之用）;*/
scrollbar-track-color: #95A6AA;/*滚动条中间可以拉的部分以外的顏色;*/
scrollbar-arrow-color: #FFFF00;/*箭头部分的顏色;*/OVERFLOW-X: Hidden;}
textarea:focus {
    outline:solid 2px #3CF;
}
