//打开添加页面
function openadd(){
	$.dialog({id:'column_add'}).close();
	$.dialog.open(addUrl, {
		title: '添加栏目',
		width: 800,
		height:400,
		lock: true,
		border: false,
		id: 'column_add',
		drag:true
	});
}

//查询功能
function search(){
	if(str_is_null($('#title').val())) {
		alert('请选择查询条件');
		return ;
	}
	var paraStr = "&title="+$('#title').val();
	$('#iframeId').attr('src',listallUrl+paraStr);
}