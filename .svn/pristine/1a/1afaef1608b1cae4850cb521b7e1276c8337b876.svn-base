
.line{width:100%;;
	height:0px;
	border-top:2px solid #dfdfe3;}
.wrapper{width:auto;
	height:auto;
	margin:0 auto;}

.logo1{width:313px;
	height:169px;
	padding-top:60px;}
/*content*/
.content{width:1000px!important;
	height:880px;
	margin-top:5px;}
.weight{color:#767070;font-weight:600;}
.buy{
	width:1000px;
	height:504px;
	margin:0 auto;
	margin-top:40px;}
.shop{width:330px;
	height:504px;
	float:left;
	margin-left:80px;}
.price{margin-top:40px;
	line-height:50px;
	width:330px;
	height:150px;
	border-bottom:dotted;}
.one{font-size:20px;color:#de2e70;}
.two{font-size:30px;color:#fe5a25;}
.three{font-size:16px;color:#7b7a7a;text-decoration:line-through;padding-left:10px;}
.inform{width:330px;height:305px;margin-top:10px;line-height:30px;font-weight:bold;}
.norms{color:#727171;font-size:14px;float:left;}
.number{padding-left:80px;font-size:14px;color:#fe5a25;}
.numbers{font-size:14px;color:#fe5a25;float:left;}
.row{width:330px;height:30px;margin-top:15px;}
.row_two{width:330px;height:30px;margin-top:35px;}
.black{float:left;font-size:14px;color:#000;font-weight:600;}
.discuss a{color:#727171;float:right;font-size:14px;}
.img_buy{width:330px;height:60px;margin-top:35px;}
.m_tip{float:right;cursor: pointer;}
.share{float:right;margin-top:30px;margin-right:70px;}
.share a{font-size:14px;color:#595858;font-weight:500;}
/*particulars*/
.particulars{height:150px;min-width:1000px;
	width:90%;margin:0 auto;}
.particulars h1{font-size:30px;color:#848484;margin-top:10px;font-weight:500;}
.information{font-size:18px;color:#727171;line-height:60px;}
.substance{min-width:1000px;
	width:90%;height:150px;font-weight:bold;margin-left:-40px;}
.substance ul{float:left;line-height:50px;}
.first{font-size:14px;color:#000;padding-left:40px;font-weight:600;}
.second{font-size:14px;color:#525252;padding-left:50px;}
.box{width:400px;height:150px;margin-left:300px;}
/*add_and_reduce*/
.count{width:210px;height:30px;float:right;}
.add, .num, .reduce {
	float: left;
	margin-right:6px;
}
.add, .reduce {
	height: 23px;
	width: 23px;
	border: 1px solid #959494;
	background: #fff;
	text-align: center;
	line-height: 23px;
	color: #959494;
	font-size:20px;
	-moz-user-select:none;/*火狐*/ -webkit-user-select:none;/*webkit浏览器*/-ms-user-select:none;/*IE10*/ -khtml-user-select:none;/*早期浏览器*/ user-select:none;}
.add:hover,.reduce:hover {
	color: #f50;
	border-color: #f60;
	cursor: pointer;
}
.num {
	width: 40px;
	height: 15px;
	line-height: 15px;
	border: 1px solid #959494;
	color: #959494;
	text-align: center;
	padding: 4px 0;
	background-color: #fff;
}
/*lunbo*/
#outer{width:460px;height:300px;float:left;position:relative;overflow:hidden;margin-top:50px;}
#inner{display:block;width:920px;height:300px;position:absolute;left:0;top:0;}
#inner li{display:block;width:460px;height:300px;float:left;font-size:72px;line-height:300px;text-align:center;text-shadow:3px 3px 2px #999,5px 5px 2px #ccc;background:#eee;}
#left,#right{ width:40px;height:100px;position:absolute;top:100px;background:#333;opacity:0.4;color:#fff;font-size:36px;text-align:center;line-height:100px;cursor:pointer; -moz-user-select:none;/*火狐*/ -webkit-user-select:none;/*webkit浏览器*/-ms-user-select:none;/*IE10*/ -khtml-user-select:none;/*早期浏览器*/ user-select:none;}
#left:hover,#right:hover{opacity:0.8}
#left{left:0;border-top-right-radius:10px;border-bottom-right-radius:10px;}
#right{right:0;border-top-left-radius:10px;border-bottom-left-radius:10px;}
#nav{width:100px;height:40px;padding-left:360px;position:absolute;bottom:0;left:0;opacity:0.8;filter:alpha(opacity=80); -moz-user-select:none;/*火狐*/ -webkit-user-select:none;/*webkit浏览器*/-ms-user-select:none;/*IE10*/ -khtml-user-select:none;/*早期浏览器*/ user-select:none;}
#nav em{display:block;float:left;width:24px;height:24px;margin-right:8px;margin-top:8px;line-height:24px;text-align:center;font-size:14px;color:#333;font-style:normal;font-family:Arial;background:#eee;border-radius:12px;cursor:pointer;}
#nav .ehover{background:#360;color:#fff;}
#outer ul li img{width:460px;height:300px;}
/*fly*/
.fly_item{
	display: block;
	width: 50px;
	height: 50px;
	border-radius: 50px;
	overflow: hidden;
	position: absolute;
	visibility: hidden;
}