.recruit_show{width:100%;margin: 0 auto;max-width: 1120px;}
.recruit_show img{margin-top:20px;max-width: 1120px;width:100%;}
.recruit_Txtbox{min-width:1000px;width:100%;height:90%;margin:0 auto;}
.recruit_nav{width:562px;height:42px;line-height:42px;margin: 0 auto;}
.recruit_nav ul{float:left;width:562px;height:42px;margin-top:30px;}
.recruit_nav ul li{float:left;width:100px;height:40px;-moz-border-radius: 10px; /* Gecko browsers */
-webkit-border-radius: 10px;   /* Webkit browsers */
border-radius:10px;            /* W3C syntax */
border:1px #ae5da1 solid;display:block;text-align:center;line-height:40px;font-size:18px;cursor:pointer; }
.recruit_nav ul li.on{border:1px #ae5da1 solid;display:block;color:#ae5da1;}
.recruit_nav ul li.index{margin-left:51px;border-color:#ff69ff;}
.recruit_nav ul li.index:hover{color:#ae5da1;border-color:#ae5da1;}
.recruit_nav ul li.index span{color:#ff69ff;}
.recruit_nav ul li.index span:hover{color:#ae5da1;}
.recruit_nav ul li span{color:#ae5da1;}
h1{width:400px;height:120px;line-height:120px;margin:0 auto;margin-top:50px;text-align:center;border-radius:10px;border:2px #ae5da1 solid;color:#cc33cc;font-size:36px;}
.responsibility{width:1000px;height:277px;margin:0 auto;background:#ffffff;-moz-border-radius: 10px;      /* Gecko browsers */
-webkit-border-radius: 10px;   /* Webkit browsers */
border-radius:10px;            /* W3C syntax */
margin-top:20px;border:#FFF 1px solid;}
.responsibility_left{width:215px;height:265px;background:#f2e5f2;-moz-border-radius: 8px;      /* Gecko browsers */
-webkit-border-radius: 8px;   /* Webkit browsers */
border-radius:8px;            /* W3C syntax */
margin-left:5px;margin-top:6px;float:left;}
.responsibility_left img{position:relative;bottom:75px;left:35px;float:left;}
ul.responsibility_text{width:488px;height:88px;margin:0 auto;margin-top:81px;margin-left:275px;}
ul.responsibility_text li{font-size:16px;font-weight:bold;color:#666666;line-height:1.7;}
.line1{border:dashed 1px #9e4897;width:70%;margin-left:260px;margin-top:80px;}
.request{width:1000px;height:277px;margin:0 auto;background:#ffffff;-moz-border-radius: 10px;      /* Gecko browsers */
    		   -webkit-border-radius: 10px;   /* Webkit browsers */
			   border-radius:10px;            /* W3C syntax */
margin-top:70px;border:#FFF 1px solid;}
.request_left{width:215px;height:265px;background:#f2e5f2;-moz-border-radius: 8px;      /* Gecko browsers */
    		   -webkit-border-radius: 8px;   /* Webkit browsers */
			   border-radius:8px;            /* W3C syntax */
margin-left:5px;margin-top:6px;float:left;}
.request_left img{position:relative;bottom:75px;left:35px;float:left;}
ul.request_text{width:488px;height:88px;margin:0 auto;margin-top:10px;margin-left:275px;}
ul.request_text li{font-size:16px;font-weight:bold;color:#666666;line-height:1.7;}
.pay{width:1000px;height:123px;margin:0 auto;background:#ffffff;-moz-border-radius: 10px;      /* Gecko browsers */
    		   -webkit-border-radius: 10px;   /* Webkit browsers */
			   border-radius:10px;            /* W3C syntax */
margin-top:70px;border:#FFF 1px solid;}
