

.center{min-width:1000px;height:auto;width:100%;margin:0 auto;font-family:'微软雅黑';}
.show{width:1000px;height:295px;margin-top:15px;margin-left: auto;margin-right: auto;}
.middle{ width:1000px;position:relative;margin:0 auto;height: auto;overflow-wrap: normal;}
.center_left{width:200px;height:auto;margin-top:30px;background:-moz-linear-gradient(top,#f6dff7,#FFF);background:-ms-linear-gradient(top,#f6dff7,#FFF);background:-webkit-linear-gradient(top,#f6dff7,#FFF);}
.center_left ul{width:200px;height:auto;margin:0;padding:0;}
.center_left ul .li_1{width:200px;height:40px;background:-moz-linear-gradient(top,#FFF,#c676d0);background:-ms-linear-gradient(top,#FFF,#c676d0);background:-webkit-linear-gradient(top,#FFF,#c676d0);text-align:center;}
.center_left ul li{width:167px;height:32px;background:url(../../images/home/images_15_professional/Titlebox.png);margin:0 auto;margin-bottom:20px;line-height:32px;}
.center_left ul .li_1 a{font-family:"宋体";font-size:22px;line-height:40px;margin-right:15px;}
.center_left ul li a{font-family:"宋体";font-size:20px;margin-left:15px;color:#000;}
.center_left ul li .red{color:#952495;font-size:22px;font-family:"黑体";margin-left:10px;}
.center_left ul li .li_2{color:#d546d5;}
.center_left ul li:hover a{color:#d546d5;}

.center_right{width:780px;position: absolute;top: 0px;left: 218px;}
.right_head{width:780px;height:40px;background:url(../../images/home/images_15_professional/titlebar.png);}
.right_head a{line-height:40px;font-family:"宋体";font-size:16px;margin-left:15px;padding-right:10px;}
.grain{margin-top:5px;float:left;}
.text_1{margin-left:10px;line-height:30px;font-size:16px;font-family:"宋体";}
.text_2{margin-left:10px;line-height:30px;font-size:16px;font-family:"宋体";margin-left:100px;}
.text_3{margin-left:10px;line-height:30px;font-size:16px;font-family:"宋体";margin-left:40px;}
.center_list{width:720px;height:auto;float:left;margin-top:-60px;}
.center_list ul{width:720px;height:auto;}
.center_list ul li{width:720px;height:60px;line-height:60px;}
.center_list ul li a{color:#000;font-size:18px;font-family:"宋体";position:relative;top:-6px;}
.center_list ul li span{color:#000;font-size:18px;font-family:"宋体";position:relative;top:-6px;float:right;margin-right:80px;}





