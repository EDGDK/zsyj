.set{float:right;font-size:14px;letter-spacing:5px;font-family:"微软雅黑";line-height:30px;}
.star{float:right;margin-right:10px;width:12px;height:8px; margin-top:11px;}
.user_right{border:#ccc 1px solid;float:right;width:220px;height:30px;font-family:"微软雅黑";font-size:12px;background:#FFF;}
input.user_right:focus{outline:solid 1px #3CF;}
input.verification:focus{outline:solid 1px #3CF;}
.icon_UserName{width:16px;height:17px;float:right;position:relative;left:215px;top:6px;}
