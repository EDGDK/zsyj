	<script>
		$(document).ready(function(){
			$(".m_tip").click(function(){
				var num=parseInt($("#count").text());
				num++;
				if(num>=1){
					$("#count").css("display","block");
				}else{
					$("#count").css("display","none");
				}
				$("#count").text(num);	
			})
			<!--微信悬浮-->	
    		$(".weixin").hover(function(){
        		$(".weixinerweima").show();
			},function(){
    			$(".weixinerweima").hide();	
			})
			/*数量加减*/	
			$(".add").click(function() {
				var n=$(this).prev().val();
				var num=parseInt(n)+1;
				if(num==0){ return;}
				$(this).prev().val(num);
				});
			$(".reduce").click(function(){
				var n=$(this).next().val();
				var num=parseInt(n)-1;
				if(num==0){ return}
				$(this).next().val(num);
			});			
		});
	</script>  
