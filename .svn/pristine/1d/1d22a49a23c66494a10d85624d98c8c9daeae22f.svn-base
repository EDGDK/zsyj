ul,li{list-style-type:none;}
a{text-decoration:none;}
body{margin:0 auto;}
img{ border:0; vertical-align:bottom;}

.center{background:#e5dde6;width:100%;min-width:1000px;margin:0 auto;}
.title{width:900px;height:30px;padding-top:30px;border-bottom:#cc00cc 1px solid;margin:0 auto;}
.title a{color:#cc00cc;line-height:30px;font-size:14px;font-family:"微软雅黑";}

.content{width:100%;min-width:1000px;height:auto;max-width:1000px;position:relative;top:60px;margin:0 auto;border-left:#FFF 1px solid;border-right:#FFF 1px solid;}

.introduce{width:550px;height:430px;background:#e5e5e5;border:#FFF 3px solid;margin-left:30px;margin-top:-40px;position:absolute;z-index:3;}
.introduce .p_1{position:relative;top:20px;left:50px;font-size:24px;font-family:"微软雅黑";}
.introduce p{font-size:18px;font-family:"微软雅黑";margin-left:80px;margin-top:30px;line-height:30px;}
.introduce ul{width:100%;text-align:left;padding-left:50px;}
.introduce ul li{width:120%;height:40px;line-height:40px;text-align:left;}
.introduce ul li a{font-size:18px;font-family:"微软雅黑";position:relative;top:4px;}
.introduce_center{width:400px;height:1%;border-bottom:#b8b8b8 solid 4px;position:relative;top:350px;left:40px;z-index:10;}
.balloon{position:relative;top:-30px;left:56%;z-index:50;}

.write{width:700px;height:320px;background:#fff6da;border:#FFF 3px solid;margin-top:20px;margin-left:10px;}
.weixin_1{width:129px;height:129px;margin-top:80px;margin-left:100px;}
.map{margin-top:-40px;margin-left:20px;}
.write_1{width:450px;height:500px;float:left;margin-left:500px;margin-top:-430px;}
.write_1 ul li{line-height:30px;margin-left:-45px;}
.write_1 ul textarea{font-size:14px;width:375px;height:204px;resize:none;background:url(../images/images_contact/Textbox_book.png);margin-top:10px;}
.write_1 ul li a{color:#996699;font-family:"微软雅黑";font-size:18px;}
.write_1 ul li .write_2{color:#996699;font-family:"微软雅黑";font-size:24px;}
.submit{width:100px;height:35px;background:#cc00cc;font-family:"微软雅黑";font-size:18px;color:#FFF;margin-top:10px;margin-left:250px;border-radius:8px;background:#cc33cc;border:none;outline:medium;}
.submit:hover{background:#ff33ff;}

.underline{width:430px;height:10px;margin-top:150px;margin-left:450px;border-bottom:#b8b8b8 solid 4px;}



