
.register{width:1000px;height:625px;margin:0 auto;margin-top:80px;}
.purple{background:url(../../images/home/images_purple/purple.png);width:450px;height:520px;margin-top:5px;float:left;}
.purple_right{float:right;margin-left:2px;width:480px;height:525px; border:#999 1px solid;box-shadow:1px 10px 10px #999;-moz-border-radius: 10px; /* Gecko browsers */-webkit-border-radius: 10px;   /* Webkit browsers */border-radius:10px;   /* W3C syntax */}
.user{width:370px;height:30px;margin-top:50px;}
