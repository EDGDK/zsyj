.all{display:none;width:100%;min-width:900px;height:100%;background:rgba(0, 0, 0, 0.3);position:absolute;top:0px;z-index:998;position:fixed;}
.window{border:#ccc 2px solid;background:#FFF;width:390px;height:200px;position:absolute;top:200px;left:500px;z-index:600;}
.window_top{width:390px;height:35px;background:#f7c1ff;border-bottom:#CCC 2px solid;}
.xinxi{line-height:35px;margin-left:20px;color:#666666;font-size:16px;font-family:"微软雅黑";}
