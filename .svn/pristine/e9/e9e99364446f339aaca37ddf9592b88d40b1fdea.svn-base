.massge_box:hover{background:url(../../images/home/images_16_Purchasepage/Select1.jpg);width:180px;height:35px;}
.num_tel{width:90px;}
.moren1{cursor: pointer;  }
.edit,.del{cursor: pointer;color: rgb(153, 102, 153);}
.shouhuo{cursor: pointer;}

