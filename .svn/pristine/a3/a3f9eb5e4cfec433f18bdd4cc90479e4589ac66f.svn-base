

	function alertTip(tips){
     $(function() {
        var main = $('<div></div>'); //创建一个父DIV
        main.attr('id', 'popup_parent'); //给父DIV设置ID
        main.addClass('main'); //添加CSS样式
 		main.css({
		  'background':'rgba(0, 0, 0, 0.3)',
		  'width':'100%',
		  'height':'100%',
		  'position':'absolute',
		  'top':'0',
		  'clear':'both',
          'position':'fixed',
		});
        var one = $('<div><button id="popup_ok" style="width:70px;height:30px;margin-left:200px;margin-top:150px;background:#f7c1ff;border:#b9b9b9 2px solid;cursor:pointer;">确认</button><button id="popup_cancel" style="width:70px;height:30px;margin-left:10px;margin-top:-30px;background:#f7c1ff;border:#b9b9b9 2px solid;cursor:pointer;">取消</button></div>'); //创建一个子DIV
        one.attr('id', 'popup_one'); //给子DIV设置ID
        one.removeClass();
        one.css({
            'margin' : '0',
            'padding' : '0',
            'width' : '390px',
            'height' : '200px',
            'position' : 'absolute',
            'margin-top' : '200px',
			'margin-left':'450px',
            'border' : '#666 2px solid',
			'z-index':'20',
        });
		
        one.appendTo(main);
 
        var two = $('<div>提示</div>');
        two.attr('id', 'popup_two');
        two.removeClass();
        two.css({
            'margin' : '0',
            'padding' : '0',
            'width' : '390px',
            'height' : '40px',
            'background-color' : '#f7c1ff',
            'margin-top' : '202px',
            'border-bottom' : '#666 2px solid',
            'margin-left' : '452px',
			'line-height':'40px',
			'text-align':'center',
			'z-index':'20',
        });
        two.appendTo(main);
 
        var three = $('<div>'+tips+'</div>');
        three.attr('id', 'popup_three');
        three.removeClass();
        three.css({
            'margin' : '0',
            'padding' : '0',
            'width' : '392px',
            'height' : '158px',
            'margin-top' : '0px',
            'margin-left' : '450px',
			'line-height':'100px',
			'text-align':'center',
			'background-color' : '#fff',
			'z-index':'20',
        });
		three.append("<img src='images/home/images_Home/sigh_ok.png' style='position:absolute;top:280px;left:500px;z-index:20;'/>")
        three.appendTo(main);
        main.appendTo('body'); //将父DIV添加到BODY中
		
		$('#popup_ok').click(function(){
		  $('#popup_parent').remove();
		});
		$('#popup_cancel').click(function(){
		  $('#popup_parent').remove();
		});
	 });
   };



